{
  "name": "next-mantine-eslint-prettier-husky-cz-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "dev": "next dev",
    "build": "next build",
    "git:commit": "cz",
    "git:push": "git push -u origin HEAD",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "find:unused": "next-unused",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@iconify/react": "^4.1.1",
    "@mantine/core": "6.0.7",
    "@mantine/dates": "6.0.7",
    "@mantine/hooks": "6.0.7",
    "@mantine/modals": "6.0.7",
    "@mantine/next": "6.0.7",
    "@mantine/notifications": "6.0.7",
    "@mantine/nprogress": "6.0.7",
    "@next/font": "^13.2.4",
    "@sentry/browser": "^7.42.0",
    "@sentry/nextjs": "^7.42.0",
    "@svgr/webpack": "^8.0.1",
    "@tabler/icons": "^1.119.0",
    "@tabler/icons-react": "^2.25.0",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-table": "^8.7.9",
    "@types/mapbox-gl": "^2.7.12",
    "@unpic/react": "^0.0.17",
    "axios": "^0.27.2",
    "commitlint": "^17.4.4",
    "dayjs": "^1.11.9",
    "graphql": "^16.7.1",
    "image-config": "^1.0.0",
    "lodash": "^4.17.21",
    "mantine-datatable": "^2.8.3",
    "mapbox-gl": "^2.15.0",
    "maplibre-gl": "^3.2.1",
    "next": "^13.2.4",
    "next-real-viewport": "^0.7.2",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-map-gl": "^7.1.3",
    "react-query": "^3.39.3",
    "recharts": "^2.7.2",
    "sharp": "^0.32.3",
    "swiper": "9.4.1",
    "unpic": "^3.0.1",
    "xstate": "^4.37.0",
    "yup": "^0.32.11",
    "zustand": "^4.3.6",
    "zustand-middleware-xstate": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@next/bundle-analyzer": "^13.2.4",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@types/node": "17.0.23",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@types/react-table": "^7.7.14",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "8.12.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "next-sitemap": "^3.1.55",
    "prettier": "^2.8.4",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.js"
    }
  }
}
